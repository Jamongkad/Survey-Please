<div class="row">
    <div class="col-md-12 col-xs-12">
        <h3>I am in a room</h3>
        <div console-box id="scrollbar1">
            <div class="scrollbar">
                <div class="track">
                    <div class="thumb">
                        <div class="end"></div>
                    </div>
                </div>
            </div>
            <div class="viewport">
                <div class="overview">
                    <p ng-repeat="msg in msgs track by $index"> 
                        <console m="msg"></console>
                    </p>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-xs-4 col-md-4">
        <h3>room</h3> 
        <p><button class="btn" ng-click="open('door')">door</button></p>
        <p><button class="btn" ng-click="open('window')">window</button></p>
        <p><button class="btn" ng-click="open('desk')">desk</button></p>
        <p ng-repeat="b in branches track by $index">
            <room-button d="b"></room-button> 
        </p>
    </div>

    <div class="col-xs-4 col-md-4">
        <h3>{{actions.room}}</h3> 
        <clicker d="actions"></clicker>
        <p><button class="btn" ng-click="scream()" ng-if="hide_scream">Scream for help.</button></p>
        <p><button class="btn" ng-click="why()" ng-hide="hide_why">What is this letter?</button></p>
    </div>
    <div class="col-xs-4 col-md-4">
        <h3>you</h3> 
        <p>Will: {{parentobj.will}}</p>
        <p>Panic: {{parentobj.panic}}</p>
        <p>Insanity: {{parentobj.insanity}}</p>
        <p>Stamina: {{parentobj.stamina}}</p>
    </div>
</div>


