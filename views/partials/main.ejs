<div class="col-md-4">
    <h3>I am in a box</h3>
    <p ng-repeat="msg in msgs track by $index"> 
        <console m="msg"></console>
    </p>
</div>

<div class="col-md-4">
    <h3>Nudge box...<span ng-show="ticks" ng-bind="ticks"></span></h3>
    <press-me></press-me>
</div>

<div ng-if="options" class="col-md-4">
    <h3>Options</h3> 
    <p><button class="btn" ng-click="scream()" ng-if="!hide_scream">Scream for help.</button></p>
</div>

